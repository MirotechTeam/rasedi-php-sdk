{
  "name": "rasedi/php-sdk",
  "description": "Unofficial PHP SDK for Rasedi payment services",
  "type": "library",
  "require": {
    "php": ">=8.1",
    "guzzlehttp/guzzle": "^7.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "Rasedi\\Sdk\\": "src/"
    },
    "files": [
      "src/Interfaces/Responses.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Rasedi\\Sdk\\Tests\\": "tests/"
    }
  }
}
